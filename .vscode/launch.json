{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Run Server Dev",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "dev"],
      "cwd": "${workspaceFolder}/server",
      "skipFiles": ["<node_internals>/**"],
      "sourceMaps": true,
      "restart": true,
      "console": "integratedTerminal"
    }
  ]
}
